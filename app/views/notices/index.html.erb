<div class="digg_pagination">
  <div clas="page_info">
    <%= page_entries_info @notices %>
  </div>
  </br>
  <div class="row">
    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10 col col-xs-offset-1 col-sm-offset-1 col-md-offset-1 col-lg-offset-1">
      <% @notices.each do |notice| %>
          <div class="panel panel-default">
            <div class="panel-body">
              <h1><%= notice.title %></h1>

              <p><%= notice.content %></p>
            </div>
          </div>
      <% end %>
    </div>
  </div>
</div>

<ul class="pagination">
  <%= will_paginate @notices%>
</ul>
<div class="panel panel-default">
  <div class="panel-body">
    <div class="blog-post">
      <h2 class="blog-post-title"><a href="<%= notice_path(notice) %>" ><%= notice.title %></a></h2>

      <p class="blog-post-meta"><%= notice.updated_at %> by
        <a href="#"><!--%= (User.find notice.author).name %--></a></p>
      <%= notice.content %>
    </div>
  </div>
</div>

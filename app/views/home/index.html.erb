
<div class="row">
  <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10 col col-xs-offset-1 col-sm-offset-1 col-md-offset-1 col-lg-offset-1">
    <div class="panel panel-default">
      <div class="panel-heading">Notice</div>
      <div class="panel-body">
        <div class="blog-main">

          <% @notices.each do |notice| %>
              <%= render partial: "notices/index",  locals: { notice: notice } %>
          <% end %>

          <div class="row">
            <div class="center">
              <ul class="pagination">
                <%= will_paginate @notices %>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
